{
  "files.eol": "\n",
  "editor.detectIndentation": false,
  "prettier.semi": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "prettier.trailingComma": "all",
  "prettier.singleQuote": true,
  "prettier.printWidth": 120,
  "prettier.tabWidth": 2,
  "prettier.endOfLine": "cr",
  "editor.tabSize": 2
}
